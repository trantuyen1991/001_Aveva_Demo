{
  "keypad": {
    "layout": {
      "default": [
        "1 2 3",
        "4 5 6",
        "7 8 9",
        "0 . {±}",
        "{bksp} {cancel} {confirm}"
      ]
    },
    "display": {
      "{±}": "±",
      "{bksp}": "⌫",
      "{confirm}": "✔",
      "{cancel}": "✖"
    }
  },
  "enhkeypad": {
    "layout": {
      "default": [
        "1 2 3",
        "4 5 6",
        "7 8 9",
        "0 . {±} {->}",
        "{bksp} {cancel} {confirm}"
      ],
      "default1": [
        "a b c",
        "d e f",
        "g h i",
        "{cap} {space} {<-} {->}",
        "{bksp} {cancel} {confirm}"
      ],
      "default2": [
        "j k l",
        "m n o",
        "p q r",
        "{cap} {space} {<-} {->}",
        "{bksp} {cancel} {confirm}"
      ],
      "default3": [
        "s t u",
        "v w x",
        "y z /",
        "{cap} {space} {<-} {->}",
        "{bksp} {cancel} {confirm}"
      ],
      "default4": [
        "# % *",
        "\\ : $",
        "+ - ,",
        "{space} {<-} {->}",
        "{bksp} {cancel} {confirm}"
      ],
      "default5": [
        "; ( )",
        "@ ! ?",
        "= > <",
        "{space} {<-} {->}",
        "{bksp} {cancel} {confirm}"
      ],
      "default6": [
        "_ . &",
        "[ ] α",
        "β γ",
        "{space} {<-}",
        "{bksp} {cancel} {confirm}"
      ],
      "default10": [
        "A B C",
        "D E F",
        "G H I",
        "{cap} {space} {<-} {->}",
        "{bksp} {cancel} {confirm}"
      ],
      "default20": [
        "J K L",
        "M N O",
        "P Q R",
        "{cap} {space} {<-} {->}",
        "{bksp} {cancel} {confirm}"
      ],
      "default30": [
        "S T U",
        "V W X",
        "Y Z /",
        "{cap} {space} {<-} {->}",
        "{bksp} {cancel} {confirm}"
      ]
    },
    "display": {
      "{<-}": "←",
      "{->}": "→",
      "{±}": "±",
      "{bksp}": "⌫",
      "{confirm}": "✔",
      "{space}": " ",
      "{cap}": "caps",
      "{cancel}": "✖"
    }
  },
  "alphanumeric": {
    "layout": {
      "default": [
        "ё 1 2 3 4 5 6 7 8 9 0 - = {bksp}",
        "{tab} й ц у к е н г ш щ з х ъ \\",
        "{cap} ф ы в а п р о л д ж э {enter}",
        "{shift} / я ч с м и т ь б ю . {shift}",
        "{alt} {space} {<-} {->} {cancel} {confirm}"
      ],
      "shift": [
        "Ё ! \" № ; % : ? * ( ) _ + {bksp}",
        "{tab} Й Ц У К Е Н Г Ш Щ З Х Ъ /",
        "{cap} Ф Ы В А П Р О Л Д Ж Э {enter}",
        "{shift} | Я Ч С М И Т Ь Б Ю , {shift}",
        "{alt} {space} {<-} {->} {cancel} {confirm}"
      ],
      "caps": [
        "ё 1 2 3 4 5 6 7 8 9 0 - = {bksp}",
        "{tab} Й Ц У К Е Н Г Ш Щ З Х ъ \\",
        "{cap} Ф Ы В А П Р О Л Д Ж Э {enter}",
        "{shift} / Я Ч С М И Т Ь Б Ю . {shift}",
        "{alt} {space} {<-} {->} {cancel} {confirm}"
      ]
    },
    "display": {
      "{bksp}": "⌫",
      "{confirm}": "✔",
      "{enter}": "⏎",
      "{space}": " ",
      "{tab}": "⇥",
      "{cap}": "caps",
      "{shift}": "shift",
      "{cancel}": "✖",
      "{<-}": "←",
      "{->}": "→",
      "{alt}": "alt"
    }
  }
}