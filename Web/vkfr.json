{
  "keypad": {
    "layout": {
      "default": [
        "1 2 3",
        "4 5 6",
        "7 8 9",
        "0 . {±}",
        "{bksp} {cancel} {confirm}"
      ]
    },
    "display": {
      "{±}": "±",
      "{bksp}": "⌫",
      "{confirm}": "✔",
      "{cancel}": "✖"
    }
  },
  "enhkeypad": {
    "layout": {
      "default": [
        "1 2 3",
        "4 5 6",
        "7 8 9",
        "0 . {±} {->}",
        "{bksp} {cancel} {confirm}"
      ],
      "default1": [
        "a b c",
        "d e f",
        "g h i",
        "{cap} {space} {<-} {->}",
        "{bksp} {cancel} {confirm}"
      ],
      "default2": [
        "j k l",
        "m n o",
        "p q r",
        "{cap} {space} {<-} {->}",
        "{bksp} {cancel} {confirm}"
      ],
      "default3": [
        "s t u",
        "v w x",
        "y z /",
        "{cap} {space} {<-} {->}",
        "{bksp} {cancel} {confirm}"
      ],
      "default4": [
        "# % *",
        "\\ : $",
        "+ - ,",
        "{space} {<-} {->}",
        "{bksp} {cancel} {confirm}"
      ],
      "default5": [
        "; ( )",
        "@ ! ?",
        "= > <",
        "{space} {<-} {->}",
        "{bksp} {cancel} {confirm}"
      ],
      "default6": [
        "_ . &",
        "[ ] α",
        "β γ",
        "{space} {<-}",
        "{bksp} {cancel} {confirm}"
      ],
      "default10": [
        "A B C",
        "D E F",
        "G H I",
        "{cap} {space} {<-} {->}",
        "{bksp} {cancel} {confirm}"
      ],
      "default20": [
        "J K L",
        "M N O",
        "P Q R",
        "{cap} {space} {<-} {->}",
        "{bksp} {cancel} {confirm}"
      ],
      "default30": [
        "S T U",
        "V W X",
        "Y Z /",
        "{cap} {space} {<-} {->}",
        "{bksp} {cancel} {confirm}"
      ]
    },
    "display": {
      "{<-}": "←",
      "{->}": "→",
      "{±}": "±",
      "{bksp}": "⌫",
      "{confirm}": "✔",
      "{space}": " ",
      "{cap}": "caps",
      "{cancel}": "✖"
    }
  },
  "alphanumeric": {
    "layout": {
      "default": [
        "` 1 2 3 4 5 6 7 8 9 0 ° + {bksp}",
        "{tab} a z e r t y u i o p ^ $",
        "{cap} q s d f g h j k l m ù * {enter}",
        "{shift} < w x c v b n , ; : ! {shift}",
        "{alt} {space} {<-} {->} {cancel} {confirm}"
      ],
      "shift": [
        "² & é \" ' ( - è _ ç à ) = {bksp}",
        "{tab} A Z E R T Y U I O P ¨ £",
        "{cap} Q S D F G H J K L M % µ {enter}",
        "{shift} > W X C V B N ? . / § {shift}",
        "{alt} {space} {<-} {->} {cancel} {confirm}"
      ],
      "caps": [
        "` 1 2 3 4 5 6 7 8 9 0 ° + {bksp}",
        "{tab} A Z E R T Y U I O P ^ $",
        "{cap} Q S D F G H J K L M ù * {enter}",
        "{shift} < W X C V B N , ; : ! {shift}",
        "{alt} {space} {<-} {->} {cancel} {confirm}"
      ]
    },
    "display": {
      "{bksp}": "⌫",
      "{confirm}": "✔",
      "{enter}": "⏎",
      "{space}": " ",
      "{tab}": "⇥",
      "{cap}": "caps",
      "{shift}": "shift",
      "{cancel}": "✖",
      "{<-}": "←",
      "{->}": "→",
      "{alt}": "alt"
    }
  }
}