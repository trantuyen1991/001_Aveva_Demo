{
  "keypad": {
    "layout": {
      "default": [
        "1 2 3",
        "4 5 6",
        "7 8 9",
        "0 . {±}",
        "{bksp} {cancel} {confirm}"
      ]
    },
    "display": {
      "{±}": "±",
      "{bksp}": "⌫",
      "{confirm}": "✔",
      "{cancel}": "✖"
    }
  },
  "enhkeypad": {
    "layout": {
      "default": [
        "1 2 3",
        "4 5 6",
        "7 8 9",
        "0 . {±} {->}",
        "{bksp} {cancel} {confirm}"
      ],
      "default1": [
        "a b c",
        "d e f",
        "g h i",
        "{cap} {space} {<-} {->}",
        "{bksp} {cancel} {confirm}"
      ],
      "default2": [
        "j k l",
        "m n o",
        "p q r",
        "{cap} {space} {<-} {->}",
        "{bksp} {cancel} {confirm}"
      ],
      "default3": [
        "s t u",
        "v w x",
        "y z /",
        "{cap} {space} {<-} {->}",
        "{bksp} {cancel} {confirm}"
      ],
      "default4": [
        "# % *",
        "\\ : $",
        "+ - ,",
        "{space} {<-} {->}",
        "{bksp} {cancel} {confirm}"
      ],
      "default5": [
        "; ( )",
        "@ ! ?",
        "= > <",
        "{space} {<-} {->}",
        "{bksp} {cancel} {confirm}"
      ],
      "default6": [
        "_ . &",
        "[ ] α",
        "β γ",
        "{space} {<-}",
        "{bksp} {cancel} {confirm}"
      ],
      "default10": [
        "A B C",
        "D E F",
        "G H I",
        "{cap} {space} {<-} {->}",
        "{bksp} {cancel} {confirm}"
      ],
      "default20": [
        "J K L",
        "M N O",
        "P Q R",
        "{cap} {space} {<-} {->}",
        "{bksp} {cancel} {confirm}"
      ],
      "default30": [
        "S T U",
        "V W X",
        "Y Z /",
        "{cap} {space} {<-} {->}",
        "{bksp} {cancel} {confirm}"
      ]
    },
    "display": {
      "{<-}": "←",
      "{->}": "→",
      "{±}": "±",
      "{bksp}": "⌫",
      "{confirm}": "✔",
      "{space}": " ",
      "{cap}": "caps",
      "{cancel}": "✖"
    }
  },
  "alphanumeric": {
    "layout": {
      "default": [
        "` 1 2 3 4 5 6 7 8 9 0 - = {bksp}",
        "{tab} ᄇ ᄌ ᄃ ᄀ ᄉ ᅭ ᅧ ᅣ ᅢ ᅦ [ ] \\",
        "{cap} ᄆ ᄂ ᄋ ᄅ ᄒ ᅩ ᅥ ᅡ ᅵ ; ' {enter}",
        "{shift} ᄏ ᄐ ᄎ ᄑ ᅲ ᅮ ᅳ , . / {shift}",
        "{alt} {space} {<-} {->} {cancel} {confirm}"
      ],
      "shift": [
        "~ ! @ # $ % ^ & * ( ) _ + {bksp}",
        "{tab} ᄈ ᄍ ᄄ ᄁ ᄊ ᅭ ᅧ ᅣ ᅤ ᅨ { } |",
        "{cap} ᄆ ᄂ ᄋ ᄅ ᄒ ᅩ ᅥ ᅡ ᅵ : {enter}",
        "{shift} ᄏ ᄐ ᄎ ᄑ ᅲ ᅮ ᅳ < > ? {shift}",
        "{alt} {space} {<-} {->} {cancel} {confirm}"
      ]
    },
    "display": {
      "{bksp}": "⌫",
      "{confirm}": "✔",
      "{enter}": "⏎",
      "{space}": " ",
      "{tab}": "⇥",
      "{cap}": "caps",
      "{shift}": "shift",
      "{cancel}": "✖",
      "{<-}": "←",
      "{->}": "→",
      "{alt}": "alt"
    }
  }
}